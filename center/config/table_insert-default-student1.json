{
  "clazz": "CommonSingleSinkFlow",
  "config": {
    "spark.dynamicAllocation.maxExecutors": "16"
  },
  "sources": [
    {
      "out": "a",
      "clazz": "HiveSource",
      "table": "default.student"
    }
  ],
  "sink": {
    "in": "a",
    "clazz": "HiveSink",
    "mode": "overwrite",
    "table": "default.student1"
  },
  "transforms": [
    {
      "clazz": "ColumnTransform",
      "ins": [
        "a"
      ],
      "columns": [
      {
        "expr": "id+1",
        "name": "id"
      }
    ]

    }
  ]
}