{
  "clazz": "CommonSingleSinkFlow",
  "config": {
    "spark.dynamicAllocation.maxExecutors": "1"
  },
  "sources": [
    {
      "clazz": "HiveSource",
      "table": "default.student"
    }
  ],
  "sink": {
    "clazz": "HiveSink",
    "mode": "overwrite",
    "table": "default.student1"
  },
  "transforms": [
    {
      "clazz": "ColumnTransform",
      "columns": [
        {
          "expr": "concat(id,name)",
          "name": "name"
        }
      ]
    }
  ]
}