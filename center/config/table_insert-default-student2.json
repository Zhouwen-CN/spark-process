{
  "clazz": "CommonSingleSinkFlow",
  "config": {
    "spark.dynamicAllocation.maxExecutors": "1"
  },
  "sources": [
    {
      "clazz": "HiveSource",
      "table": "default.student"
    },
    {
      "clazz": "HiveSource",
      "table": "default.address"
    }
  ],
  "sink": {
    "clazz": "HiveSink",
    "mode": "overwrite",
    "table": "default.student2"
  },
  "transforms": [
    {
      "clazz": "JoinTransform",
      "cs": "l",
      "condition": "l.id = r.id"
    },
    {
      "clazz": "ColumnTransform",
      "columns": [
        {
          "expr": "city",
          "name": "address"
        }
      ]
    }
  ]
}